<div class="container">
  <div class="row">
    <div class="col-3"></div>
    <div class="col-8">
      <h1>Users</h1>
      <table class="table">
        <thead>
          <tr>
            <th>image</th>
            <th>name</th>
          </tr>
          <tbody>
            <% @users.each do |user| %>
              <tr>
                <td></td>
                <td><%= user.name %></td>
              </tr>
            <% end %>
          </tbody>
        </thead>
      </table>
    </div>
  </div>
</div>